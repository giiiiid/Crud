{
    "builds":[{
        "src":"challenge/wsgi.py",
        "use":"@vercel/python",
        "config": {"maxLambdaSize":"15mb", "runtime":"python3.11"}
    }],
    "routes": [{
        "src":"/(.*)",
        "dest":"challenge/wsgi.py"
    }],
    "functions": [{
        "api/test.js": {
          "memory": 3008,
          "maxDuration": 30 
        },
        "api/*.js": {
          "memory": 3008,
          "maxDuration": 15
        }
      }]
}

